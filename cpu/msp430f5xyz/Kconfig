config CPU_FAM_MSP430F5
    bool
    select CPU_CORE_MSP430
    select HAS_CPU_MSP430F5XYZ

## CPU Models
config CPU_MODEL_MSP430F5438A
    bool
    select CPU_FAM_MSP430F5

## Definition of specific features
config HAS_CPU_MSP430F5XYZ
    bool
    help
        Indicates that an 'msp430f5xyz' cpu is being used.

## Common CPU symbols
config CPU_FAM
    default "msp430f5" if CPU_FAM_MSP430F5

config CPU_MODEL
    default "msp430f5438a" if CPU_MODEL_MSP430F5438A

config CPU
    default "msp430f5xyz" if CPU_FAM_MSP430F5

source "$(RIOTCPU)/msp430_common/Kconfig"
